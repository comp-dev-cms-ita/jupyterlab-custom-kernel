{
  "name": "@deshaw/jupyterlab-ksmm",
  "version": "0.1.7",
  "description": "An extension to manage Kernelspecs from JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ipython",
    "kernel"
  ],
  "homepage": "https://github.com/deshaw/ksmm",
  "bugs": {
    "url": "https://github.com/deshaw/ksmm/issues"
  },
  "license": "BSD-3-Clause",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/deshaw/ksmm"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:all": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf ksmm/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "rimraf": "./node_modules/rimraf/bin.js",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter-labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0",
    "@jupyterlab/coreutils": "^5.1.0",
    "@jupyterlab/launcher": "^3.1.0",
    "@jupyterlab/services": "^6.1.0",
    "@rjsf/bootstrap-4": "^5.2.1",
    "@rjsf/core": "^5.2.1",
    "@types/react-jsonschema-form": "1.7.8",
    "core-js": "^3.29.0",
    "react": "^17.0.2",
    "react-bootstrap": "2.7.2",
    "react-dom": "^17.0.2",
    "react-jsonschema-form": "1.8.1",
    "rimraf": "4.4.0"
  },
  "devDependencies": {
  },
  "resolutions": {
    "**/@types/react": "18.0.28"
  },
  "resolutions": {
    "**/@jupyterlab/application": "3.1.0",
    "**/@jupyterlab/apputils": "3.1.0",
    "**/@jupyterlab/cells": "3.1.0",
    "**/@jupyterlab/codemirror": "3.1.0",
    "**/@jupyterlab/completer": "3.1.0",
    "**/@jupyterlab/console": "3.1.0",
    "**/@jupyterlab/coreutils": "5.1.0",
    "**/@jupyterlab/docmanager": "3.1.0",
    "**/@jupyterlab/docregistry": "3.1.0",
    "**/@jupyterlab/documentsearch": "3.1.0",
    "**/@jupyterlab/filebrowser": "3.1.0",
    "**/@jupyterlab/json-extension": "3.1.0",
    "**/@jupyterlab/launcher": "3.1.0",
    "**/@jupyterlab/mainmenu": "3.1.0",
    "**/@jupyterlab/mathjax2": "3.1.0",
    "**/@jupyterlab/notebook": "3.1.0",
    "**/@jupyterlab/rendermime": "3.1.0",
    "**/@jupyterlab/services": "6.1.0",
    "**/@jupyterlab/settingregistry": "3.1.0",
    "**/@jupyterlab/terminal": "3.1.0",
    "**/@jupyterlab/theme-dark-extension": "3.1.0",
    "**/@jupyterlab/theme-light-extension": "3.1.0",
    "**/@jupyterlab/ui-components": "3.1.0",
    "**/@lumino/commands": "1.15.0",
    "**/@lumino/coreutils": "1.11.1",
    "**/@lumino/default-theme": "0.14.0",
    "**/@lumino/widgets": "1.23.0",
    "**/@jupyter-widgets/base": "5.0.0-beta.1",
    "**/@jupyter-widgets/controls": "4.0.0-beta.1",
    "**/@jupyter-widgets/html-manager": "0.21.0-beta.1",
    "**/@jupyter-widgets/jupyterlab-manager": "4.0.0-beta.1",
    "**/@jupyter-widgets/output": "5.0.0-beta.1",
    "**/@mui/material": "5.0.0",
    "**/@mui/icons-material": "5.0.0",
    "**/@mui/styles": "5.0.0",
    "**/@mui/system": "5.0.0",
    "**/@mui/lab": "5.0.0-alpha.47",
    "**/@types/react": "17.0.6",
    "**/@types/react-dom": "17.0.5",
    "**/react": "17.0.2",
    "**/react-dom": "17.0.2",
    "**/react-redux": "7.2.4",
    "**/redux": "4.1.0",
    "**/redux-observable": "1.2.0",
    "**/rxjs": "6.5.5",
    "**/typescript": "4.2.4",
    "**/html-webpack-plugin": "5.3.1",
    "**/webpack": "5.57.1",
    "**/webpack-cli": "4.9.0",
    "**/webpack-dev-server": "4.3.1"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "ksmm/labextension",
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "ksmm"
        }
      }
    }
  }
}
